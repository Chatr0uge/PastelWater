      }
    },
    {
      "scope": "string.escape",
      "settings": {
        "foreground": "#761a18"
      }
    },
    {
      "scope": "string.quasi",
      "settings": {
        "foreground": "#3a7476"
      }
    },
    {
      "scope": "string.entity",
      "settings": {
        "foreground": "#7f625e"
      }
    },
    {
      "scope": "object",
      "settings": {
        "foreground": "#382b36"
      }
    },
    {
      "scope": "module.node",
      "settings": {
        "foreground": "#3a7476"
      }
    },
    {
      "scope": "support.type.property-name",
      "settings": {
        "foreground": "#76797c"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#761a18"
      }
    },
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#761a18"
      }
    },
    {
      "scope": "keyword.control.module",
      "settings": {
        "foreground": "#3a7476"
      }
    },
    {
      "scope": "keyword.control.less",
      "settings": {
        "foreground": "#fa805e"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#3a7476"
      }
    },
    {
      "scope": "keyword.operator.new",
      "settings": {
        "foreground": "#a8352e"
      }
    },
    {
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#7f625e"
      }
    },
    {
      "scope": "metatag.php",
      "settings": {
        "foreground": "#a8352e"
      }
    },
    {
      "scope": "support.function.git-rebase",
      "settings": {
        "foreground": "#76797c"
      }
    },
    {
      "scope": "constant.sha.git-rebase",
      "settings": {
        "foreground": "#7f625e"
      }
    },
    {
      "name": "Types declaration and references",
      "scope": [
        "meta.type.name",
        "meta.return.type",
        "meta.return-type",
        "meta.cast",
        "meta.type.annotation",
        "support.type",
        "storage.type.cs",
        "variable.class"
      ],
      "settings": {
        "foreground": "#c85647"
      }
    },
    {
      "scope": ["variable.this", "support.variable"],
      "settings": {
        "foreground": "#904344"
      }
    },
    {
      "scope": [
        "entity.name",
        "entity.static",
        "entity.name.class.static.function",
        "entity.name.function",
        "entity.name.class",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#c85647"
      }
    },
    {
      "name": "Function declarations",
      "scope": ["entity.function", "entity.name.function.static"],
      "settings": {
        "foreground": "#3a7476"
      }
    },
    {
      "scope": "entity.name.function.function-call",
      "settings": {
        "foreground": "#3a7476"
      }
    },
    {
      "scope": "support.function.builtin",
      "settings": {
        "foreground": "#a8352e"
      }
    },
    {
      "scope": [
        "entity.name.method",
        "entity.name.method.function-call",
        "entity.name.static.function-call"
      ],
      "settings": {
        "foreground": "#76797c"
      }
    },
    {
      "scope": "brace",
      "settings": {
        "foreground": "#55424a"
      }
    },
    {
      "name": "variables",
      "scope": [
        "meta.parameter.type.variable",
        "variable.parameter",
        "variable.name",
        "variable.other",
        "variable",
        "string.constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#3a7476"
      }
    },
    {
      "scope": "prototype",
      "settings": {
        "foreground": "#904344"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#7f625e"
      }
    },
    {
      "scope": "punctuation.quoted",
      "settings": {
        "foreground": "#382b36"
      }
    },
    {
      "scope": "punctuation.quasi",
      "settings": {
        "foreground": "#761a18"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    // ----------------------------------------
    //            LANGUAGE SPECIFIC
    // ----------------------------------------
    // PYTHON ----------------------------------------
    {
      "name": "Python function",
      "scope": ["meta.function.python", "entity.name.function.python"],
      "settings": {
        "foreground": "#3a7476"
      }
    },
    {
      "name": "Python Function and Class definition keywords",
      "scope": [
        "storage.type.function.python",
        "storage.modifier.declaration",
        "storage.type.class.python",
        "storage.type.string.python"
      ],
      "settings": {
        "foreground": "#761a18"
      }
    },
    {
      "name": "Async keyword",
      "scope": ["storage.type.function.async.python"],
      "settings": {
        "foreground": "#761a18"
      }
    },
    {
      "name": "Python Function Call",
      "scope": "meta.function-call.generic",
      "settings": {
        "foreground": "#3a7476"
      }
    },
    {
      "name": "Python Function Arguments",
      "scope": "meta.function-call.arguments",
      "settings": {
        "foreground": "#55424a"
      }
    },
    {
      "name": "Python Function decorator",
      "scope": "entity.name.function.decorator",
      "settings": {
        "foreground": "#c85647",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Python ALL CAPS",
      "scope": "constant.other.caps",
      "settings": {
        "fontStyle": "bold"
      }
    },
    // SHELL ----------------------------------------
    {
      "scope": "keyword.operator.logical",
      "settings": {
        "foreground": "#761a18"
      }
    },
    {
      "scope": "punctuation.definition.logical-expression",
