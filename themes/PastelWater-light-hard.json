{
  "$schema": "vscode://schemas/color-theme",
  "name": "Gruvbox Light Hard",
  "semanticHighlighting": true,
  "tokenColors": [
    {
      "settings": {
        "foreground": "#382b36"
      }
    },
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "header",
      "settings": {
        "foreground": "#3a7476"
      }
    },
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#a27771",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant", "support.constant", "variable.arguments"],
      "settings": {
        "foreground": "#904344"
      }
    },
    {
      "scope": "constant.rgb-value",
      "settings": {
        "foreground": "#382b36"
      }
    },
    {
      "scope": "entity.name.selector",
      "settings": {
        "foreground": "#3a7476"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#c85647"
      }
    },
    {
      "scope": ["entity.name.tag", "punctuation.tag"],
      "settings": {
        "foreground": "#3a7476"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#a8352e"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#a27771"
      }
    },
    {
      "scope": "meta.selector",
      "settings": {
        "foreground": "#3a7476"
      }
    },
    {
      "scope": "meta.preprocessor",
      "settings": {
        "foreground": "#a8352e"
      }
    },
    {
      "scope": "meta.preprocessor.string",
      "settings": {
        "foreground": "#7f625e"
      }
    },
    {
      "scope": "meta.preprocessor.numeric",
      "settings": {
        "foreground": "#7f625e"
      }
    },
    {
      "scope": "meta.header.diff",
      "settings": {
        "foreground": "#a8352e"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#761a18"
      }
    },
    {
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#a8352e"
      }
    },
    {
      "scope": "string",
